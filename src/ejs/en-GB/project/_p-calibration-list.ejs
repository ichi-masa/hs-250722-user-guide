<div class="p-lower-list">
  <ul class="p-lower-list__items">
    <li class="p-lower-list__item">
      <p class="p-lower-list__lead"><%= json.calibration.title %></p>
      <ul class="p-lower-list__sub-items">
        <% const calibrationItems = [
          { href: './calibration-selecting-the-personal-number-manually-before-measurement.html', text: 'Selecting the Personal Number Manually Before Measurement' },
          { href: './calibration-transferring-the-readings-later.html', text: 'Transferring the Readings Later' },
          { href: './calibration-measuring-without-recording.html', text: 'Measuring without Recording (GUEST mode)' },
          { href: './calibration-changing-or-deleting-your-personal-data.html', text: 'Changing or Deleting Your Personal Data' },
          { href: './calibration-replacing-the-batteries.html', text: 'Replacing the Batteries' },
          { href: './calibration-restoring-the-unit-to-factory-settings.html', text: 'Restoring the Unit to Factory Settings' },
          { href: './calibration-performing-initial-settings-on-the-unit.html', text: 'Performing Initial Settings on the Unit' },
          { href: './calibration-registering-personal-data-on-the-unit.html', text: 'Registering Personal Data on the Unit' }
        ]; %>
        <% calibrationItems.forEach(item => { %>
        <li class="p-lower-list__sub-item">
          <a href="<%= item.href %>" class="p-lower-list__sub-link p-lower-list__sub-link--storage"><span><%= item.text %></span></a>
        </li>
        <% }); %>
      </ul>
    </li>
  </ul>
</div>